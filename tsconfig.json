{
  "compilerOptions": {
    "target": "es2016",
    "module": "es6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "moduleResolution": "Node"
  },
  "include": [
    "./**/*"
  ],
  "plugins": [
    {"marked": "marked/marked.min.js"}
  ]
}
