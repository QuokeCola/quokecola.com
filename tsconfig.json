  "compilerOptions": {
    "target": "es2016",
    "module": "es6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "allowJs": true,
    "moduleResolution": "Node",
    "outDir": "./lib"
  },
  "include": [
    "./**/*.ts"
  ],
  "plugins": [
